#include<iostream>

using namespace std;

main(){
	int a,b,c;
	cout<<"Digite o valor de a: ";
	cin>>a;
	
	cout<<"Digite o valor de b:";
	cin>>b;
	
	cout<<"Digite o valor de c:";
	cin>>c;
	
	if ((a>b) and (a>c))
		cout<<"'a' eh o maior "<<endl;
	return 0;
		
		if(b>c)
			cout<<"'b' eh o maior"<<endl;
		else
			cout<<"'c' eh o maior"<<endl;
}
